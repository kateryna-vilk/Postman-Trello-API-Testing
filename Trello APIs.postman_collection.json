		{
			"name": "Create Board",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json();",
							"",
							"pm.environment.set(\"boardID\", jsonData.id);",
							"",
							"",
							"// Test assertions to verify required properties exist",
							"pm.test(\"Check if 'id' exists\", function () {",
							"    pm.expect(jsonData.id).to.exist;",
							"});",
							"",
							"pm.test(\"Check if 'name' exists\", function () {",
							"    pm.expect(jsonData.name).to.exist;",
							"});",
							"",
							"pm.test(\"Check if 'url' exists\", function () {",
							"    pm.expect(jsonData.url).to.exist;",
							"});",
							"",
							"let createdboardname = pm.environment.get(\"myboardname\");",
							"",
							"pm.test(\"Correct Board Name\", function () {",
							"    pm.expect(jsonData.name).to.eql(createdboardname);",
							"});",
							"",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let randomNum = Math.floor(Math.random() * 1000).toString().padStart(3, '0');\r",
							"\r",
							"let uniqueValue = `MyFirstBoard-${randomNum}`;\r",
							"\r",
							"pm.environment.set(\"myboardname\", uniqueValue);"
						],
